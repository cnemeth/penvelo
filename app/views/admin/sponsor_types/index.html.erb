<h1>Listing Sponsor Types</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Created At</th>
    <th>Updated At</th>
  </tr>

<% @sponsor_types.each do |sponsor_type| %>
  <tr>
    <td><%=h sponsor_type.id %></td>
    <td><%=h sponsor_type.name %></td>
    <td><%=h sponsor_type.created_at %></td>
    <td><%=h sponsor_type.updated_at %></td>

    <td><%= link_to 'Show', admin_sponsor_type_path(sponsor_type) %></td>
    <td><%= link_to 'Edit', edit_admin_sponsor_type_path(sponsor_type) %></td>
    <td><%= link_to "Delete", admin_sponsor_type_path(sponsor_type), :confirm => 'Are you sure?',:method => :delete  %></td>
  </tr>
<% end %>
</table>

<p>
  <%= link_to 'Create New Sponsor Type', new_admin_sponsor_type_path %>
</p>

