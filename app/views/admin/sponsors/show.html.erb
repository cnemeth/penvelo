<h1>Sponsor</h1>

<table>
<tr><th>ID:</th><td><%=h @sponsor.id %></td></tr>
<tr><th>Name:</th><td><%=h @sponsor.name %></td></tr>
<tr><th>Phone</th><td><%=h @sponsor.phone %></td></tr>
<tr><th>Email</th><td><%=h @sponsor.email %></td></tr>
<tr><th>URL</th><td><%=h @sponsor.url %></td></tr>
<tr><th>Logo URL</th><td><%=h @sponsor.logo_url %></td></tr>
<tr><th>Contact name</th><td><%=h @sponsor.contact_name %></td></tr>
