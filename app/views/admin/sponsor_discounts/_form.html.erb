<% semantic_form_for([:admin, @sponsor_discount]) do |f| %>
  <%= f.error_messages %>
  <% f.inputs do %>

      <%= f.input :discount, :label => 'Discount' %>
      <%= f.input :sponsor_id, :label => 'Sponsor', :as => :select, :collection => sponsors %>

  <% end %>
  <%= f.submit "Submit" %>
<% end %>

