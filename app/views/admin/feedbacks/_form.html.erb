<% semantic_form_for([:admin, @feedback]) do |f| %>
  <%= f.error_messages %>
  <% f.inputs do %>

      <%= f.input :feedback, :label => 'Feedback' %>
      <%= f.input :feedback_type, :label => 'Feedback type', :as => :select, :collection => feedback_types, :include_blank => false %>

  <% end %>
  <%= f.submit "Submit" %>
<% end %>

