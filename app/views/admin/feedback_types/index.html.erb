<h1>Listing Feedback Types</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Created At</th>
    <th>Updated At</th>
  </tr>

<% @feedback_types.each do |feedback_type| %>
  <tr>
    <td><%=h feedback_type.id %></td>
    <td><%=h feedback_type.name %></td>
    <td><%=h feedback_type.created_at %></td>
    <td><%=h feedback_type.updated_at %></td>

    <td><%= link_to 'Show', admin_feedback_type_path(feedback_type) %></td>
    <td><%= link_to 'Edit', edit_admin_feedback_type_path(feedback_type) %></td>
    <td><%= link_to "Delete", admin_feedback_type_path(feedback_type), :confirm => 'Are you sure?',:method => :delete  %></td>
  </tr>
<% end %>
</table>

<p>
  <%= link_to 'Create New Feedback Type', new_admin_feedback_type_path %>
</p>

