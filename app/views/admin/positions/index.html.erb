<h1>Listing Club Positions</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Created At</th>
    <th>Updated At</th>
  </tr>

<% @positions.each do |position| %>
  <tr>
    <td><%=h position.id %></td>
    <td><%=h position.name %></td>
    <td><%=h position.created_at %></td>
    <td><%=h position.updated_at %></td>

    <td><%= link_to 'Show', admin_position_path(position) %></td>
    <td><%= link_to 'Edit', edit_admin_position_path(position) %></td>
    <td><%= link_to "Delete", admin_position_path(position), :confirm => 'Are you sure?',:method => :delete  %></td>
  </tr>
<% end %>
</table>

<p>
  <%= link_to 'Create New Position', new_admin_position_path %>
</p>

