<% @clubs = Club.find(:all) %>
<% for club in @clubs -%>

<div id="header_1">

	<div id="club_logo">
		<%= image_tag(club.logo_url) %>
	</div>

	<div id="club_dates"><em>Est. <%= club.year %></em></div>

</div>

<div id="header_main">

	<div id="headline">
		<h1><%= club.name  %> Admin Page</h1>
	</div>
	<div id="curent_date_time">
		<h2>Current date and time: <%= Time.now %></h2>
	</div>
	<div id="search">
		<form method="get" action="">
			<input class="iform" type="text"  size="25" maxlength="256" name="query" value="... search query ..." />
			<input type="submit" value="Search" class="button" />
		</form>
	</div>
</div>



<% end %>

